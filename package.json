{
	"name": "sk-sesher",
	"version": "0.0.5",
	"description": "Simple password-based session management for SvelteKit personal projects",
	"author": "Neville Brem",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nevthereal/sk-sesher.git"
	},
	"homepage": "https://github.com/nevthereal/sk-sesher#readme",
	"bugs": {
		"url": "https://github.com/nevthereal/sk-sesher/issues"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./auth/remote": {
			"types": "./dist/auth.remote.d.ts",
			"svelte": "./dist/auth.remote.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.46.0",
		"@sveltejs/kit": "^2.49.0"
	},
	"dependencies": {
		"valibot": "^1.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.5",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"publint": "^0.3.15",
		"svelte": "^5.46.4",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.6"
	},
	"keywords": [
		"svelte",
		"sveltekit",
		"session",
		"authentication",
		"auth",
		"password",
		"remote-functions"
	]
}
